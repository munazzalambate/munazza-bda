import pdfplumber
with pdfplumber.open('cleaned.pdf') as pdf:
page = pdf.pages[0] 
table = page.extract_table() 
for rows in table:
print(rows)

PRACTICAL04
import pandas as pd, sqlite3
df = pd.read_csv("car_data.csv").dropna()
conn = sqlite3.connect("car.db")
df.to_sql("car", conn, if_exists="replace", index=False)
cursor = conn.cursor()
for row in cursor.execute("SELECT * FROM car LIMIT 10"):
    print(row)
cursor.execute("SELECT sql FROM sqlite_master WHERE type='table' AND name='car'")
print("\nSQL used to create the table:\n", cursor.fetchone()[0])
conn.close()

PRACTICAL05
Code: import pandas as pd 
import re 
df = pd.read_excel("RowData.xlsx")
df = df.drop_duplicates().dropna()
def clean(text):
if isinstance(text,str):
text = re.sub(r'\s+','',text) 
text = re.sub(r'[^\w\s.,-]','',text) 
text.strip()  
return text
df = df.map(clean)
df.to_excel("clean.xlsx",index=False)
print(df)

PRACTICAL06
!pip install pandas matplotlib --quiet
import pandas as pd
data = {
'name': [
'Aarti', 'Baban', 'Chetan', 'Daya', 'aarushi',
'sanika', 'Geeta', 'Hemant', 'Ira', 'Jay',
'Karan', 'munazza', 'Mira', 'prem', 'anuja',
'Payal', 'aadi', 'Riya', 'ram', 'reena'
],
'math': [
78, 92, 65, 85, 70,
88, 60, 95, 72, 80,
68, 91, 73, 77, 83,
69, 86, 75, 66, 89
],
'science': [
83, 88, 70, 90, 75,
91, 65, 96, 78, 85,
71, 89, 74, 79, 87,
72, 90, 77, 68, 93
]
}
df = pd.DataFrame(data) ;df.to_csv('students_scores.csv', index=False) 
pd.read_csv('students_scores.csv')
print("Student Scores Table:\n") 
print(df)
correlation = df['math'].corr(df['science'])
print(f"\n Correlation between Math and Science scores: {correlation:.2f}")
import matplotlib.pyplot as plt
plt.figure(figsize=(8, 5))
plt.scatter(df['math'], df['science'], color='purple', alpha=0.7) 
plt.xlabel("Math Score")
plt.ylabel("Science Score") 
plt.title("Math vs Science Score Correlation") 
plt.grid(True)
plt.show()

PRACTICAL 07
For Scatter Plot
import pandas as pd
import matplotlib.pyplot as plt
df = pd.read_excel(&quot;corruption_vs_child_labour.xlsx&quot;)
plt.figure(figsize=(8, 5))
plt.scatter(df["Perceived Corruption Score (0-100)"], df["Child Labour(%)"], color='blue')
for i in range(len(df)):
plt.text(df.iloc[i, 1]+0.5, df.iloc[i, 2]+0.5, df.iloc[i, 0],fontsize=9)
plt.title(&quot;Perceived Corruption vs Child Labour&quot;)
plt.xlabel("Perceived Corruption Score (0 = most corrupt, 100 = leastcorrupt)")
plt.ylabel("Child Labour (%)")
plt.grid(True)
plt.tight_layout()
plt.show()

For Multiplebars
import pandas as pd import 
matplotlib.pyplot as plt import 
numpy as np
df = pd.read_excel("corruption_vs_child_labour.xlsx")
x = np.arange(len(df["Country"]))
width = 0.4
plt.figure(figsize=(12,6))
plt.bar(x - width/2, df["Perceived Corruption Score (0-100)"], width, label='Corruption Score')
plt.bar(x + width/2, df["Child Labour(%)"], width, label='Child Labour %')
# Labeling plt.xlabel("Country") 
plt.ylabel("Score / Percentage")
plt.title("Perceived Corruption vs Child Labour by Country") 
plt.xticks(ticks=x, labels=df["Country"], rotation=45, ha='right') 
plt.legend() plt.tight_layout()
plt.grid(axis='y')
plt.show()


PRACTICAL08
import pandas as pd import 
plotly.express as px
# Load CSV data df = 
pd.read_csv("Child_labour.csv")
# Create choropleth world map fig 
= px.choropleth(
df,
locations="Code", # ISO Alpha-3 country codes 
color="Child Labour %", # values to show 
hover_name="Country", # hover info 
color_continuous_scale="Reds", # color scheme title="Child Labour 
Worldwide (%)"
)
# Save as HTML file
fig.write_html("child_labour_map.html") print("Map 
saved as child_labour_map.html")
