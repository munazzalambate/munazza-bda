PRACTICAL04
import pandas as pd, sqlite3
df = pd.read_csv("car_data.csv").dropna()
conn = sqlite3.connect("car.db")
df.to_sql("car", conn, if_exists="replace", index=False)
cursor = conn.cursor()
for row in cursor.execute("SELECT * FROM car LIMIT 10"):
    print(row)
cursor.execute("SELECT sql FROM sqlite_master WHERE type='table' AND name='car'")
print("\nSQL used to create the table:\n", cursor.fetchone()[0])
conn.close()

PRACTICAL05
import pandas as pd
import re
df = pd.read_excel(&quot;childmarriage.xlsx&quot;)
df = df.drop_duplicates().dropna()
def clean(text):
if isinstance(text, str):
text = re.sub(r&#39;\s+&#39;, &#39;&#39;, text) 
text = re.sub(r&#39;[^\w\s.,-]&#39;, &#39;&#39;, text)
return text.strip()
return text
df = df.applymap(clean)
df.to_excel(&quot;cleaned.xlsx&quot;, index=False)

PRACTICAL06
!pip install pandas matplotlib --quiet
import pandas as pd
data = {
'name': [
'Aarti', 'Baban', 'Chetan', 'Daya', 'aarushi',
'sanika', 'Geeta', 'Hemant', 'Ira', 'Jay',
'Karan', 'munazza', 'Mira', 'prem', 'anuja',
'Payal', 'aadi', 'Riya', 'ram', 'reena'
],
'math': [
78, 92, 65, 85, 70,
88, 60, 95, 72, 80,
68, 91, 73, 77, 83,
69, 86, 75, 66, 89
],
'science': [
83, 88, 70, 90, 75,
91, 65, 96, 78, 85,
71, 89, 74, 79, 87,
72, 90, 77, 68, 93
]
}
df = pd.DataFrame(data) ;df.to_csv('students_scores.csv', index=False) 
pd.read_csv('students_scores.csv')
print("Student Scores Table:\n") 
print(df)
correlation = df['math'].corr(df['science'])
print(f"\n Correlation between Math and Science scores: {correlation:.2f}")
import matplotlib.pyplot as plt
plt.figure(figsize=(8, 5))
plt.scatter(df['math'], df['science'], color='purple', alpha=0.7) 
plt.xlabel("Math Score")
plt.ylabel("Science Score") 
plt.title("Math vs Science Score Correlation") 
plt.grid(True)
plt.show()

PRACTICAL 07
For Scatter Plot
import pandas as pd
import matplotlib.pyplot as plt
df = pd.read_excel(&quot;corruption_vs_child_labour.xlsx&quot;)
plt.figure(figsize=(8, 5))
plt.scatter(df["Perceived Corruption Score (0-100)"], df["Child Labour(%)"], color='blue')
for i in range(len(df)):
plt.text(df.iloc[i, 1]+0.5, df.iloc[i, 2]+0.5, df.iloc[i, 0],fontsize=9)
plt.title(&quot;Perceived Corruption vs Child Labour&quot;)
plt.xlabel("Perceived Corruption Score (0 = most corrupt, 100 = leastcorrupt)")
plt.ylabel("Child Labour (%)")
plt.grid(True)
plt.tight_layout()
plt.show()

For Multiplebars
import pandas as pd
import matplotlib.pyplot as plt
import numpy as np
# Load the data
df = pd.read_excel(&quot;corruption_vs_child_labour_20rows.xlsx&quot;)
# Set position of bars on X axis
x = np.arange(len(df[&quot;Country&quot;]))
width = 0.4
# Plotting
plt.figure(figsize=(12, 6))
plt.bar(x - width/2, df[&quot;Perceived Corruption Score (0-100)&quot;], width,
label=&#39;Corruption Score&#39;)
plt.bar(x + width/2, df[&quot;Child Labour (%)&quot;], width, label=&#39;Child Labour
%&#39;)
# Labeling
plt.xlabel(&quot;Country&quot;)
plt.ylabel(&quot;Score / Percentage&quot;)
plt.title(&quot;Perceived Corruption vs Child Labour by Country&quot;)
plt.xticks(ticks=x, labels=df[&quot;Country&quot;], rotation=45, ha=&#39;right&#39;)
plt.legend()
plt.tight_layout()
plt.grid(axis=&#39;y&#39;)
# Show plot
plt.show()


PRACTICAL08
import pandas as pd
import plotly.express as px
df = pd.read_csv(&quot;child_labour.csv&quot;)
fig = px.choropleth(
df,
locations=&quot;Code&quot;, 
color=&quot;Child Labour %&quot;, 
hover_name=&quot;Country&quot;,
color_continuous_scale=&quot;Reds&quot;, 
title=&quot;Child Labour Worldwide (%)&quot;
)
fig.write_html(&quot;child_labour_map.html&quot;)
print(&quot;Map saved as child_labour_map.html&quot;)

8B
import requests
def get_summary(title):
url = f&quot;https://en.wikipedia.org/api/rest_v1/page/summary/{title}&quot;
headers = {&quot;User-Agent&quot;: &quot;MyBot/1.0 (https://example.com/contact)&quot;}
r = requests.get(url, headers=headers)
if r.ok:
data = r.json()
print(f&quot;{data[&#39;title&#39;]}:\n{data[&#39;extract&#39;]}\n&quot;)
else:
print(f&quot;Error {r.status_code}: Could not fetch summary.&quot;)
get_summary(&quot;OpenAI&quot;)
get_summary(&quot;Python_(programming_language)&quot;)
