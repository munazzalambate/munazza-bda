8B
import requests 
def get_summary(title): 
 url = f"https://en.wikipedia.org/api/rest_v1/page/summary/{title}" 
headers = {"User-Agent": "MyBot/1.0 (https://example.com/contact)"} 
r = requests.get(url, headers=headers) if r.ok: 
 data = r.json() 
print(f"{data['title']}:\n{data['extract']}\n") else: 
 print(f"Error {r.status_code}: Could not fetch summary.") 
# Example calls get_summary("OpenAI") 
get_summary("Python_(programming_language)")
